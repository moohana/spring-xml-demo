<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="actor" class="com.stackroute.domain.Actor">
        <constructor-arg  value="priya"></constructor-arg>
        <constructor-arg value="female"></constructor-arg>
        <constructor-arg value="23"></constructor-arg>
    </bean>
    <bean id="movie" autowire="byName" class="com.stackroute.domain.Movie">

    </bean>
    <bean id="actor1" class="com.stackroute.domain.Actor">
        <property   name="name" value="priya"></property>
        <property  name="gender" value="female"></property>
        <property  name ="age" value="23"></property>
    </bean>
    <bean id="mov" autowire="constructor" class="com.stackroute.domain.Movie">

    </bean>
    <bean id="beanlife" class="com.stackroute.domain.BeanLifecycleDemoBean" init-method="customInit"
          destroy-method="customDestroy">
        <property name="message" value="i'm property message" />
    </bean>
    <bean id="beanlife1" class="com.stackroute.domain.BeanPostProcessorDemoBean">
    <property name="beanName" value="this is the post processor"/>
    </bean>


</beans>